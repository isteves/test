name: check-version-bump
on:
  pull_request:
    branches:
      - main

jobs:
  docit:
    runs-on: ubuntu-latest

    steps:
    - uses: actions/checkout@v2
    - name: Verify Changed files
        uses: tj-actions/verify-changed-files@v8.8
        id: verify-changed-files
        with:
          files: |
             test/setup.py
